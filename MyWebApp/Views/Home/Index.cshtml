@model List<Student>;
@{
    ViewData["Title"] = "Home Page";
}

<table class="table">
    <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">First</th>
            <th scope="col">Last</th>
            <th scope="col">Mobile</th>
            <th scope="col">Email</th>
        </tr>
    </thead>
    <tbody>
        
        @foreach(var item in Model)
        {
           <tr>
                <th scope="row">@item.Id</th>
                <td>@item.FirstName</td>
                <td>@item.LastName</td>
                <td>@item.Mobile</td>
                <td>@item.Email</td>
            </tr> 
        }

        
    </tbody>
</table>